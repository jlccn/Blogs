<?xml version="1.0" encoding="utf-8"?>

<!--
  有关如何配置 ASP.NET 应用程序的详细消息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
  <appSettings>
    <add key="SQLiteDbNameForWeb" value="App_Data/DB.db3" />
  </appSettings>
  <connectionStrings>    
    <add name="Conn" connectionString="Data Source=|DataDirectory|DB.db3;Pooling=true;FailIfMissing=false" providerName="System.Data.SQLite"/>
  </connectionStrings>
    <system.web>
        <httpRuntime requestValidationMode="2.0" />
        <compilation debug="true" targetFramework="4.0" />
        <customErrors mode="Off"/>
    </system.web>
</configuration>
